{
  "name": "@pnpm-mono/file",
  "version": "1.0.0",
  "main": "./build/main.js",
  "types": "./build/main.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build && rimraf coverage",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "npm run test:ci",
    "test:ci": "jest --ci --collectCoverage --maxWorkers 2 --passWithNoTests"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/archiver": "^5.3.2",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "jest": "29.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "rootDir": "./build",
    "displayName": "@pnpm-mono/file",
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "statements": 5,
        "branches": 5,
        "functions": 5,
        "lines": 5
      }
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.304.0",
    "@aws-sdk/lib-storage": "^3.304.0",
    "@aws-sdk/s3-request-presigner": "^3.304.0",
    "@pnpm-mono/config": "workspace:*",
    "archiver": "^5.3.1",
    "xlsx-js-style": "^1.2.0"
  }
}